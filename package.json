{
  "name": "@smashing/form",
  "version": "0.6.1",
  "description": "MobX powered forms in React",
  "main": "dist/index.js",
  "module": "dist/form.esm.js",
  "typings": "dist/index.d.ts",
  "author": "EYEDEA AS",
  "repository": "eyedea-io/smashing-form",
  "bugs": {
    "url": "https://github.com/eyedea-io/smashing-form/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "react-dom",
    "form",
    "validation",
    "forms",
    "react-hook",
    "hook",
    "smashing"
  ],
  "homepage": "https://github.com/eyedea-io/smashing-form",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "prebuild": "rm -rf dist",
    "test": "tsdx test --env=jsdom",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "mobx": ">=5",
    "mobx-react-lite": ">=1",
    "react": ">=16.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@testing-library/react": "^9.1.0",
    "@types/jest": "^24.0.17",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/yup": "^0.26.22",
    "husky": "^3.0.3",
    "mobx": "^5.13.0",
    "mobx-react-lite": "^1.4.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "size-limit": "^1.3.8",
    "tsdx": "^0.7.2",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3",
    "yup": "^0.27.0"
  },
  "dependencies": {
    "dot-prop": "^5.1.0",
    "react-fast-compare": "^2.0.4"
  },
  "size-limit": [
    {
      "path": "./dist/index.js",
      "limit": "3 kB"
    },
    {
      "path": "./dist/form.esm.js",
      "limit": "3 kB"
    },
    {
      "path": "./dist/form.cjs.production.min.js",
      "limit": "3 kB"
    }
  ]
}
